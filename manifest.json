{
  "name": "Screenshotter",
  "description": "Captures page screenshots",
  "version": "0.7",
  "permissions": [
    "debugger"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Capture Screenshot"
  },
  "manifest_version": 2
}